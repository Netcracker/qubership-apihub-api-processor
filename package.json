    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "allof-merge": "0.5.1",
    "api-ref-bundler": "0.4.0",
    "fast-equals": "5.0.1",
    "graphql": "16.9.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "object-hash": "^3.0.0",
    "slug": "^8.2.3",
    "swagger2openapi": "^7.0.8"
  },
  "devDependencies": {
    "@netcracker/qubership-apihub-npm-gitflow": "3.1.0",
    "@types/adm-zip": "0.5.7",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.5",
    "@types/mime-types": "^3.0.0",
    "@types/node": "^18.0.0",
    "@types/object-hash": "^3.0.2",
    "@types/slug": "^5.0.3",
    "@types/swagger2openapi": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "axios": "^1.4.0",
    "eslint": "^8.22.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "mime-types": "^3.0.1",
    "openapi-types": "^12.1.0",
    "ts-jest": "29.1.2",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.6.2",
    "vite": "5.2.6",
    "vite-plugin-dts": "3.6.4",
    "vite-plugin-singlefile": "2.0.1",
    "vite-plugin-target": "0.1.1"
  },
  "eslintConfig": {
    "extends": ".eslintrc.json"
  },
  "engines": {
    "node": ">=18.0.0"
