{
  "openapi": "3.0.0",
  "info": {
    "title": "Wildcard and Param Paths",
    "version": "1.0.0"
  },
  "servers": [
    { "url": "/" }
  ],
  "paths": {
    "/path/v1/path/**": {
      "get": {
        "responses": {
          "200": { "description": "ok" }
        }
      }
    },
    "/path/v1/path/{id}": {
      "get": {
        "parameters": [
          { "name": "id", "in": "path", "required": true, "schema": { "type": "string" } }
        ],
        "responses": {
          "200": { "description": "ok" }
        }
      }
    },
    "/path/v1/path/*/{*id}": {
      "get": {
        "parameters": [
          { "name": "*id", "in": "path", "required": true, "schema": { "type": "string" } }
        ],
        "responses": {
          "200": { "description": "ok" }
        }
      }
    },
    "/path/v1/path/{id}/": {
      "get": {
        "parameters": [
          { "name": "id", "in": "path", "required": true, "schema": { "type": "string" } }
        ],
        "responses": {
          "200": { "description": "ok" }
        }
      }
    }
  }
}

